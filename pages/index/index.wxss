/**index.wxss**/
.main-wrapper{
  height: 100vh;
}

.bg-logo{
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 18vh;
  filter: blur(15rpx);
  z-index: -1;
}

.bg-logo>image{
  width: 100%;
  height: 100%;
}

.top{
  background: #0af;
  height: 8vh;
  justify-content: flex-start;
  padding-right: 5vw;
  padding-top: 2vh;
}

.distance{
  color: white;
  margin-left: 5vw;
  line-height: 6vh;
}

.top>input{
  flex: 1;
  background: white;
  margin-left: 3vw;
  border-radius: 3vw;
  font-size: 25rpx;
  height: 6vh;
  padding: 0 5vw;
}


.header{
  position: relative;
  height: 18vh;
  padding: 2vh 3vh;
  justify-content: flex-start;
  background-color: rgba(7,17,27,0.5);
}

.logo{
  width: 20vw;
  height: 20vw;
  margin-left: 2vw;
}

.shopinfo{
  height: 20vw;
  font-size: 30rpx;
  color: white;
  flex: 1;
  margin-left: 3vw;
}

.title{
  font-size: 30rpx
}

.title:before{
  content: '';
  background-image: url(data:image/png;base64,);
  width: 9vw;
  height: 5vw;
  background-size: cover;
  display: inline-block;
  vertical-align: top;
  margin-right: 8rpx;
}

.address{
  font-size: 25rpx;
}

.tel{
  font-size: 25rpx;
}

.info{
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 4vh;
  line-height: 4vh;
  font-size: 20rpx;
  color: white;
  padding-left: 2vw;
  box-sizing: border-box;
  background: rgba(7,17,27,0.2);
}

.announcement{
  display: inline-block;
}

.announcement:before{
  content: '';
  width: 5.5vw;
  height: 3vw;
  vertical-align: -3rpx;
  margin-right: 8rpx;
  display: inline-block;
  background-size: cover;
  background-image: url(data:image/png;base64,);
}

.content-wrapper{
  height: 68vh;
  overflow: hidden;
}

.leftnav{
  width: 20vw;
  background: #f3f5f7;
  font-size: 27rpx;
  overflow: auto;
  height: 100%;
  flex-basis: 20vw;
}

.leftnav-li{
  height: 9vh;
  padding: 2vh 0 0 5vw;
  justify-content: center;
  border-bottom: 1rpx solid rgba(7,17,27,0.1);
}

.left_avtive{
  background: white;
}

.brief-list{
  flex: 1;
  padding: 2vw 0 2vw 2vw;
}

.goods-detail{
  height: 10vh;
  margin-bottom: 5vh;
  padding-bottom: 3vh;
  border-bottom: 1rpx solid rgba(7,17,27,0.1);
}

.goods-image{
  width: 15vw;
  height: 15vw;
}

.text{
  flex: 1;
  margin-left: 2vw;
}

.brief-name{
  font-size: 30rpx;
}

.brief-description{
  font-size: 22rpx;
}

.brief-price{
  font-size: 25rpx;
  color: red;
}